sno=int(input("enter sno"))
sname=input("Enter student name")
group=input("Enter student group")
s1=int(input("Enter data science marks"))
s2=int(input("Enter statistics marks"))
s3=int(input("Enter computers marks"))
total=s1+s2+s3
avg=total/3
if avg>=90:
    print("O-Grade")
elif avg>=80:
    print("A-Grade")
elif avg>=70:
    print("B-Grade")
elif avg>=60:
    print("C-Grade")
elif avg>=50:
    print("D-Grade")
else:
    print("pass")
